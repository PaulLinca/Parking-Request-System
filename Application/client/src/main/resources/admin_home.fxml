<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewmodel.AdminHomeController">
    <children>
        <ImageView fitHeight="610.0" fitWidth="810.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="home2.png" />
            </image>
        </ImageView>
        <Button fx:id="logoutButton" layoutX="661.0" layoutY="27.0" mnemonicParsing="false" onAction="#logoutAction" prefHeight="50.0" prefWidth="125.0" text="Logout" />
      <Button fx:id="assignButton" layoutX="338.0" layoutY="27.0" mnemonicParsing="false" onAction="#assignAction" prefHeight="25.0" prefWidth="125.0" text="Assign spot" />
      <Button fx:id="retractButton" layoutX="338.0" layoutY="52.0" mnemonicParsing="false" onAction="#retractAction" prefHeight="25.0" prefWidth="125.0" text="Retract Spot" />
      <Button fx:id="viewLotsButton" layoutX="29.0" layoutY="28.0" mnemonicParsing="false" onAction="#viewLotsAction" prefHeight="50.0" prefWidth="125.0" text="View Parking Lots" />
      <Button fx:id="viewSpotsButton" layoutX="186.0" layoutY="27.0" mnemonicParsing="false" onAction="#viewSpotsAction" prefHeight="25.0" prefWidth="125.0" text="View Lot Spots" />
      <ListView fx:id="listView" layoutX="116.0" layoutY="195.0" prefHeight="356.0" prefWidth="589.0" visible="false" />
      <Button fx:id="exitButton" layoutX="361.0" layoutY="581.0" mnemonicParsing="false" onAction="#exitAction" text="Exit Application" />
      <Button fx:id="clearButton" layoutX="116.0" layoutY="169.0" mnemonicParsing="false" onAction="#clearAction" text="Clear" visible="false" />
      <ComboBox fx:id="spotsCombo" layoutX="186.0" layoutY="78.0" prefHeight="25.0" prefWidth="125.0" promptText="Choose lot" />
      <ComboBox fx:id="requestCombo" layoutX="338.0" layoutY="78.0" prefHeight="25.0" prefWidth="125.0" promptText="Choose lot" />
      <Button fx:id="viewRequestsButton" layoutX="186.0" layoutY="52.0" mnemonicParsing="false" onAction="#viewRequestsAction" prefHeight="25.0" prefWidth="125.0" text="View Lot Requests" />

    </children>
</AnchorPane>
