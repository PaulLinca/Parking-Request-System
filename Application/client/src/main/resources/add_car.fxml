      </ImageView>
      <Label layoutX="112.0" layoutY="186.0" text="Add new car" textAlignment="CENTER" textFill="#1cd7c4" textOverrun="CLIP">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="vinField" layoutX="46.0" layoutY="241.0" prefHeight="25.0" prefWidth="238.0" promptText="Car's VIN" />
      <TextField fx:id="ptiField" layoutX="46.0" layoutY="291.0" prefHeight="25.0" prefWidth="238.0" promptText="Car's PTI" />
   </children>
</AnchorPane>
